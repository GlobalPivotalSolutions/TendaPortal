<script>
   jQuery(document).ready(function($) {
   
   $('.post .post_thumb img').imgresize({parent : '.post_thumb',fade : 1000}); 
   
      $('div.actions').live("click", function() {
         var btn = this;
         var data = {
             action :'update_like_action',
             postid: this.id,
             userid: 0,
             nonce : 'd84b952f64'
         };
            
         $.ajax({
            async: false,
            type: 'POST',
            url: 'http://ebola.prepperdailynews.net/wp-admin/admin-ajax.php',
            data: data,
            dataType: 'json',
            success: function(data) { 
                                      $('.post-likes-num', btn).html(data.likes);
                                      if(data.userid > 0) { 
                                          $(btn).addClass('actions-liked').removeClass('actions');
                                         $('.post-save-label', btn).html('Liked');
                                      }
                                    }
            
         });
      });

      $('div.save-actions').click(function() {
         alert('Please sign into Ebola Outbreak Watch\n\nIf you are not a member already, please register above.');
      });

$('div.save-actions-delete').click(function() {
   var btn = this;
   var data = {
       action :'delete_save_action',
       postid: this.id,
       userid: 0,
       nonce : 'bd39d2779a'
   };
      
   $.ajax({
      async: false,
      type: 'POST',
      url: 'http://ebola.prepperdailynews.net/wp-admin/admin-ajax.php',
      data: data,
      dataType: 'json',
      success: function(data) { $('#savedstories-count').html(data.message);
                                $(btn).parents('.reading_list').remove();}
   });
});



   });
</script>

